/**
 * @file    C1_Balteus_1_0_0.c
 * @brief   Application entry point.
 */
#include <stdio.h>

#include "../NXP/board/board.h"
#include "../NXP/board/clock_config.h"
#include "../NXP/board/peripherals.h"
#include "../NXP/board/pin_mux.h"
#include "../NXP/device/LPC55S69_cm33_core1.h"
#include "../NXP/utilities/fsl_debug_console.h"
/* TODO: insert other include files here. */

/* TODO: insert other definitions and declarations here. */

/*
 * @brief   Application entry point.
 */
int main(void) {


    PRINTF("Hello World from SLAVE\r\n");

    /* Force the counter to be placed into memory. */
    volatile static int i = 0 ;
    /* Enter an infinite loop, just incrementing a counter. */
    while(1) {
        i++ ;
        /* 'Dummy' NOP to allow source level single stepping of
            tight while() loop */
        __asm volatile ("nop");
    }
    return 0 ;
}
